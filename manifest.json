{
  "description": "AWS SAML relay to localhost webserver + tweetnacl encryption",
  "manifest_version": 3,
  "name": "saml-secure-relay",
  "version": "2.0.0",
  "homepage_url": "https://github.com/francisg-onfido/saml-secure-relay",
  "icons": {
    "48": "pen.svg"
  },

  "permissions": [
    "webRequest",
    "storage"
  ],

  "host_permissions": [
    "https://signin.aws.amazon.com/*",
    "http://127.0.0.1:31415/*"
  ],

  "background": {
    "service_worker": "service_worker.js"
  }
}
